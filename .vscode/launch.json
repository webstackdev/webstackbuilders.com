{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "eleventy-build",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/eleventy",
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}",
      "env": { "ELEVENTY_ENV": "production", "DEBUG": "Eleventy*" },
      "console": "internalConsole",
      "runtimeVersion": "17.5.0"
    },
    {
      "name": "eleventy-serve",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/eleventy",
      "stopOnEntry": false,
      "args": ["--watch"],
      "cwd": "${workspaceRoot}",
      "env": { "ELEVENTY_ENV": "development" },
      "console": "internalConsole",
      "runtimeVersion": "17.5.0"
    },
    {
      "name": "gulp-task",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/gulp",
      "stopOnEntry": false,
      "args": ["build:html:debug"],
      "cwd": "${workspaceRoot}",
      "env": { "TS_NODE_PROJECT": "tsconfig.gulp.json" },
      "console": "internalConsole",
      "runtimeVersion": "17.5.0"
    },
    {
      "name": "jest-single-test",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "stopOnEntry": false,
      "args": [
        "eleventy/filters/__tests__/excerpt.spec.js", "--projects=test/jest/jest.config.node.ts"
      ],
      "cwd": "${workspaceRoot}",
      "env": { "TS_NODE_PROJECT": "tsconfig.gulp.json" },
      "console": "internalConsole",
      "runtimeVersion": "17.5.0"
    }
  ]
}
