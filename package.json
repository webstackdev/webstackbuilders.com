    "info": "This repo uses a Yarn plugin to launch scripts as Gulp tasks.\n",
    "build": "Generate a production build for deployment",
    "build:css": "Compile SCSS to CSS in the output directory",
    "build:html": "Compile HTML templates to the output directory using 11ty",
    "build:html:debug": "Run 11ty CLI in debug mode",
    "build:lambda": "Generate a bundle of serverless functions code for Netlify",
    "build:script": "Compile client-side TypeScript using Webpack",
    "build:serviceworker": "Generate a service worker using Workbox",
    "build:social-images": "Generates social share images from each generated page using Puppeteer",
    "build:social-styles": "Generates CSS for social share images",
    "build:sprites": "Builds an SVG sprite file and compiles it to _layouts/images/sprites.njk",
    "check:frontmatter": "Validate front matter in page directory templates",
    "check:dependencies": "Run Yarn Doctor to check for dependency problems",
    "format": "Run all format subtasks",
    "format:js": "Check and fix JavaScript files with Prettier, then ESLint",
    "format:json": "heck and fix JSON files with Prettier",
    "format:sass": "Check and fix SCSS with Stylelint",
    "start": "Start a development server with file watchers and hot module reloading",
    "start:debug": "Start a HMR dev server with watchers and 11ty's DEBUG flag set",
    "start:testing": "Build a website with 'testing' env var set and serve locally",
    "test": "Run all tests",
    "test:coverage": "Generate test coverage metrics to /coverage/ directory",
    "test:e2e": "Launches Playwright and runs end-to-end tests",
    "test:unit": "Launches the Jest test runner",
    "test:unit:ci": "Runs Jest tests serially and errors (not updates) on empty snapshots",
    "test:unit:update": "Update snapshots",
    "util:bump": "Update the 'version' tag in package.json using semantic versioning",
    "util:clean": "Delete build artifact directories /public/ and /social/",
    "util:release": ""
  },
