{
  "name": "webstack-builders-corporate-website-eleventy",
  "version": "0.0.1",
  "description": "This is the corporate website for Webstack Builders, implemented with Eleventy",
  "keywords": [],
  "author": "Kevin Brown <kevin@webstackbuilders.com> (https://webstackbuilders.com/)",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/webstackbuilders/webstack-builders-corporate-website-eleventy"
  },
  "bugs": {
    "url": "https://github.com/webstackbuilders/webstack-builders-corporate-website-eleventy/issues"
  },
  "node": ">=17.1.0",
  "scripts": {
    "prebuild": "run-s clean setup",
    "build": "run-s build:script build:css build:html",
    "build:css": "gulp build:css",
    "build:script": "webpack --config webpack.config.prod.js",
    "build:html": "cross-env ELEVENTY_ENV=production eleventy",
    "postbuild": "run-s postbuild:*",
    "postbuild:social-images": "npx eleventy-social-images --outputDir public --dataFile social/pages.json --templatePath social/template.html",
    "postbuild:social-styles": "gulp postbuild:social-styles",
    "bump": "npm --no-git-tag-version version",
    "clean": "gulp clean",
    "debug": "DEBUG=* npx eleventy",
    "format": "run-p format:*",
    "format:js": "prettier '**/*.js' --write && eslint '**/*.js' --fix",
    "format:json": "prettier '**/*.json' --write",
    "format:sass": "stylelint --fix 'src/assets/css/**/*.scss' --custom-syntax postcss-scss",
    "serve": "npm run build && serve ./public/",
    "setup": "gulp setup",
    "prestart": "run-s clean setup build:script build:css",
    "start": "cross-env ELEVENTY_ENV=development run-p watch:*",
    "watch:script": "webpack --config webpack.config.dev.js",
    "watch:css": "gulp watch:css",
    "watch:html": "ELEVENTY_ENV=development npx eleventy --watch --serve --incremental",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --runInBand",
    "test:cy:open": "cypress open",
    "test:cy:run": "npx npm-package-scripts run-cypress",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "//": "stylelint is pinned at ^9.1.0 by dependencies",
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@11ty/eleventy-img": "2.0.0",
    "@11ty/eleventy-plugin-directory-output": "1.0.1",
    "@11ty/eleventy-plugin-inclusive-language": "1.0.3",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@11tyrocks/eleventy-plugin-social-images": "^0.4.1",
    "@babel/core": "7.17.9",
    "@babel/preset-typescript": "7.16.7",
    "@types/eslint": "8.4.1",
    "@types/jest": "27.4.1",
    "@types/markdown-it": "12.2.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-jest": "27.5.1",
    "core-js": "3.22.0",
    "cross-env": "7.0.3",
    "cssnano": "^5.1.7",
    "cypress": "9.5.4",
    "date-fns": "2.28.0",
    "del": "6.0.0",
    "eleventy-plugin-lazyimages": "2.1.2",
    "eslint": "8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsdoc": "^39.2.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fancy-log": "2.0.0",
    "gulp": "4.0.2",
    "gulp-postcss": "9.0.1",
    "gulp-sass": "5.1.0",
    "gulp-sourcemaps": "3.0.0",
    "html-minifier": "4.0.0",
    "jest": "27.5.1",
    "luxon": "2.3.2",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.6.2",
    "markdown-it-emoji": "2.0.0",
    "md5-file": "5.0.0",
    "mkdirp": "1.0.4",
    "npm-run-all": "^4.1.5",
    "outdent": "0.8.0",
    "postcss": "^8.4.12",
    "postcss-preset-env": "7.4.3",
    "postcss-svgo": "5.1.0",
    "prettier": "2.6.2",
    "sass": "1.50.0",
    "serve": "13.0.2",
    "stylelint": "^14.7.1",
    "stylelint-a11y": "1.2.3",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "stylelint-images": "2.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "terser": "5.12.1",
    "terser-webpack-plugin": "5.3.1",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0",
    "yargs": "17.4.1"
  },
  "bin": {
    "npm-package-scripts": "./scripts/npm-package-scripts"
  },
  "browserslist": [
    "> 1%",
    "not IE 11"
  ]
}
