  "scripts-info": {
    "info": "This repo uses a Yarn plugin to launch scripts as Gulp tasks.\n",
    "build": "Generate a production build for deployment",
    "build:css": "Compile SCSS to CSS in the output directory",
    "build:css-critical": "Inline critical CSS into head of HTML files in output dir",
    "build:fonts": "Optimize and copy font files over to the output directory",
    "build:html": "Compile HTML templates to the output directory using 11ty",
    "build:html:compress": "Compress 11ty HTML output in build dir",
    "build:html:debug": "Run 11ty CLI in debug mode",
    "build:html:minify": "Minify 11ty HTML output in build dir",
    "build:images": "Optimize and copy site images over to the output directory",
    "build:lambda": "Generate a bundle of serverless functions code for Netlify",
    "build:script": "Compile client-side TypeScript using Webpack",
    "build:service-worker": "Generate a service worker using Workbox",
    "build:social-images": "Generates social share images from each generated page using Puppeteer",
    "build:social-styles": "Generates CSS for social share images",
    "build:sprites": "Builds an SVG sprite file and compiles it to _layouts/images/sprites.njk",
    "format": "Run all format subtasks",
    "format:json": "Fix JSON files with Prettier",
    "format:sass": "Fix SCSS with Stylelint",
    "format:script": "Fix JavaScript files with Prettier, then ESLint",
    "lint": "Run all linting subtasks",
    "lint:frontmatter": "Validate front matter in page directory templates",
    "lint:sass": "Check SCSS with Stylelint",
    "lint:script": "Check JavaScript files with Prettier, then ESLint",
    "start": "Start a development server with file watchers and hot module reloading",
    "start:testing": "Build a website with 'testing' env var set and serve locally",
    "stats:html": "Calculate detailed stats when generating Eleventy HTML bundle",
    "stats:script": "Show output bundle statistics generated with Webpack bundle analyzer",
    "test": "Run all test subtasks",
    "test:coverage": "Generate test coverage metrics to /coverage/ directory",
    "test:e2e": "Launches Playwright and runs end-to-end tests",
    "test:e2e:codegen": "Playwright will generate the code from user interactions in a browser session",
    "test:e2e:report": "Launch an interactive web page with the results of the Playwright tests",
    "test:e2e:update-browsers": "Update the browser executables used by Playwright",
    "test:unit": "Launches the Jest test runner",
    "test:unit:ci": "Runs Jest tests serially and errors (not updates) on empty snapshots",
    "test:unit:clear-cache": "Clears Jest's cache used in watch mode from the project .cache directory",
    "test:unit:update-snapshot": "Update snapshots with Jest",
    "util:clean": "Delete build artifact directories /public/ and /social/",
    "util:release": "",
    "util:setup": "Create necessary directories in the build output target",
    "watch": "Run all watch subtasks",
    "watch:css": "Watch SCSS files and recompile on change",
    "watch:html": "Watch 11ty source files and recompile on change",
    "watch:lambda": "Watch lambda function source files and recompile on change",
    "watch:script": "Watch client-side script files and recompile on change",
    "watch:test:unit": "Run Jest unit tests when source files change"
  },
