{
  "name": "webstackbuilders",
  "version": "0.0.1",
  "title": "Webstack Builders Company Website",
  "description": "This is the company website for Webstack Builders",
  "organization": "Webstack Builders",
  "author": "Kevin Brown <kevin@webstackbuilders.com> (https://webstackbuilders.com/)",
  "domain": "webstackbuilders.com",
  "email": "kevin@webstackbuilders.com",
  "lang": "en",
  "locale": "en_US",
  "keywords": [],
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/webstackbuilders/webstack-builders-corporate-website-eleventy",
  "bugs": {
    "url": "https://github.com/webstackbuilders/webstack-builders-corporate-website-eleventy/issues"
  },
  "node": ">=17.1.0",
  "scripts": {
    "prebuild": "run-s util:clean util:setup --print-label",
    "build": "dotenv -c -- run-s build:sprites build:script build:css build:html --print-label",
    "build:css": "gulp build:css",
    "build:html": "npx @11ty/eleventy",
    "build:lambda": "netlify-lambda build lambda",
    "build:script": "webpack --progress --profile --config webpack.config.prod.js",
    "build:sprites": "gulp build:sprites",
    "postbuild": "run-s postbuild:social-images postbuild:social-styles postbuild:serviceworker --print-label",
    "postbuild:serviceworker": "gulp build:serviceworker",
    "postbuild:social-images": "npx eleventy-social-images --outputDir public --dataFile social/pages.json --templatePath social/template.html",
    "postbuild:social-styles": "gulp postbuild:social-styles",
    "format": "run-p format:*",
    "format:js": "prettier '**/*.js' --write && eslint '**/*.js' --fix",
    "format:json": "prettier '**/*.json' --write",
    "format:sass": "stylelint --fix 'src/assets/css/**/*.scss' --custom-syntax postcss-scss",
    "preserve": "yarn run prebuild",
    "serve": "yarn run build",
    "postserve": "yarn run postbuild",
    "start": "dotenv -c -v ELEVENTY_ENV=development -- yarn run watch",
    "start:debug": "DEBUG=Eleventy* yarn run start",
    "start:testing": "dotenv -c -v ELEVENTY_ENV=testing -- yarn run build  && dotenv -c -v ELEVENTY_ENV=testing -- cross-var serve -l %ELEVENTY_TESTING_SERVER_PORT% ./public/",
    "prewatch": "yarn run prebuild && run-s build:sprites build:css --print-label",
    "watch": "run-p watch:css watch:html watch:script --print-label",
    "watch:css": "gulp watch:css",
    "watch:html": "cross-var npx @11ty/eleventy --watch --serve --incremental --port=%ELEVENTY_DEV_SERVER_PORT%",
    "watch:lambda": "yarn run build:lambda",
    "watch:script": "webpack --progress --profile --config webpack.config.dev.js",
    "postwatch": "run-s postbuild:social-images postbuild:social-styles --print-label",
    "test": "run-s test:coverage test:e2e",
    "test:e2e": "yarn run start:testing && playwright test",
    "test:unit": "jest",
    "test:unit:ci": "jest --ci --runInBand",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:update": "jest --updateSnapshot",
    "util:bump": "yarn version --no-git-tag-version",
    "util:clean": "gulp clean",
    "util:release": "np --no-cleanup",
    "util:setup": "gulp setup"
  },
