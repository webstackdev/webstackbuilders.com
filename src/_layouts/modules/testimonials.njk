---
pagination:
  data: globalDataSet.testimonials
  size: 6
  alias: testimonials
---
{# This is the home page module for testimonials #}
<ol>
{% for testimonial in testimonials %}
  <li>
    <a href="{{ testimonial.slug | url }}">{{ testimonial.data.name }}</a>
    <p>{{ testimonial.data.description }}</p>
  </li>
{% endfor %}
</ol>
