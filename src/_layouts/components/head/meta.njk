<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1" />
<title>{% pageTitle pageTitle=this.page.title %}</title>
<meta name="description" content="{% pageDescription pageDescription=this.page.description %}" />
<meta name="author" content="{{ author.name }}">
{#-  -#}
{#- % include 'seo.njk' %#}
{#- Open Graph meta tags for Twitter and Facebook social shares -#}
{% filter indent(2, true) %}{% include "./social.njk" %}{% endfilter %}
{#- Favicons using 'eleventy-favicon' plugin -#}
{% favicon buildPaths.faviconSvgSourceFilename %}
{#- Web manifest file for Progressive Web Application app usage #}
<link rel="manifest" href="{{ '/manifest.json' | url }}">
{#- Meta key/value pair for the theme switcher, queriable as 'meta[name="theme-color"]' #}
<meta name="theme-color" content="#FFFFFF">
{#- RSS Feed #}
<link
  type="application/rss+xml"
  rel="alternate"
  href="{{ '/feed.xml' | absoluteUrl }}"
  title="RSS Feed for {{ site.title }}"
>
{#- Webmentions #}
<link rel="pingback" href="https://webmention.io/{{ site.domain }}/xmlrpc">
<link rel="webmention" href="https://webmention.io/{{ site.domain }}/webmention">
<link rel="canonical" href="{% canonical page %}">