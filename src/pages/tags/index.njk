---
pagination:
  data: collections
  size: 1
  alias: tag
filter:
  - articles
  - case-studies
  - services
  - site
  - testimonials
layout: layouts/base.njk
permalink: /tags/{{ tag }}/
---
<h1>Tagged "{% normalizeTagName tag %}"</h1>
<ol>
{% set taglist = collections[ tag ] %}
{% for post in taglist | reverse %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ol>